{
    "AWSEBDockerrunVersion": "2",
    "containerDefinitions": [
        {
            "name": "client",
            "image": "vmun1x/multi-client",
            "hostname": "client",
            "essential": "false"
        },
        {
            "name": "server",
            "image": "vmun1x/multi-server",
            "hostname": "api",
            "essential": "false"
        },
        {
            "name": "worker",
            "image": "vmun1x/multi-worker",
            "hostname": "worker",
            "essential": "false"
        },
        {
            "name": "nginx",
            "image": "vmun1x/multi-nginx",
            "hostname": "nginx",
            "essential": true,
            "portMappings": [
                {
                "hostPort": 80,
                "containerPort": 20
                }
            ],
            "links": [
                "client",
                "server"
            ]

        }
    ]
}